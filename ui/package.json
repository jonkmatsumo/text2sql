{
  "name": "text2sql-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "gen:types": "npm run gen:types:otel && npm run gen:types:ui && npm run gen:types:agent",
    "gen:types:check": "npm run gen:types && git diff --exit-code src/gen",
    "gen:types:otel": "openapi-typescript src/gen/otel_openapi.json -o src/gen/otel_types.ts",
    "gen:types:ui": "openapi-typescript src/gen/ui_openapi.json -o src/gen/ui_types.ts",
    "gen:types:agent": "openapi-typescript src/gen/agent_openapi.json -o src/gen/agent_types.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.2",
    "vitest": "^4.0.18"
  }
}
