{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "EvaluationMetricsV1",
    "type": "object",
    "properties": {
        "run_id": {
            "type": "string"
        },
        "timestamp": {
            "type": "number"
        },
        "config": {
            "type": "object"
        },
        "exact_match_count": {
            "type": "integer"
        },
        "exact_match_rate": {
            "type": "number"
        },
        "avg_structural_score": {
            "type": "number"
        },
        "min_structural_score": {
            "type": "number"
        },
        "dataset_source": {
            "type": "string"
        },
        "total_cases": {
            "type": "integer"
        },
        "successful_cases": {
            "type": "integer",
            "description": "Deprecated: use exact_match_count"
        },
        "failed_cases": {
            "type": "integer",
            "description": "Deprecated"
        },
        "accuracy": {
            "type": "number",
            "description": "Deprecated: use exact_match_rate"
        },
        "avg_latency_ms": {
            "type": "number"
        },
        "p95_latency_ms": {
            "type": "number"
        }
    },
    "required": [
        "run_id",
        "timestamp",
        "total_cases",
        "exact_match_count",
        "exact_match_rate",
        "avg_structural_score"
    ]
}
