[run]
source = streamlit
omit =
    */tests/*
    */__pycache__/*
    */\.streamlit/*
    app.py  # Exclude UI layer

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
